{
  "name": "aunty-cli",
  "version": "1.3.0",
  "description": "The aunty command line interface",
  "license": "UNLICENSED",
  "private": true,
  "repository": "abcnews/aunty-cli",
  "contributors": [
    "Colin Gourlay <gourlay.colin@abc.net.au>"
  ],
  "bin": {
    "aunty": "bin/aunty.js"
  },
  "scripts": {
    "build-lib": "rm -rf lib/* && babel src --out-dir lib --presets=es2017",
    "postinstall": "npm run build-lib",
    "start": "chokidar 'src' --command 'npm run build-lib' --initial --silent"
  },
  "engines": {
    "node": ">=4.2.0"
  },
  "dependencies": {
    "babel-cli": "^6.22.2",
    "babel-preset-es2017": "^6.22.0",
    "chalk": "^1.1.3",
    "jsonfile": "^2.4.0",
    "minimist": "^1.2.0",
    "node-version": "^1.0.0",
    "rsyncwrapper": "^2.0.1",
    "ssh2": "^0.5.4",
    "vinyl-fs": "^2.4.4",
    "vinyl-ftp": "^0.5.0"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0"
  }
}
