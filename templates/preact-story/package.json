{
    "name": "{{projectName}}",
    "version": "1.0.0",
    "description": "A Preact-enabled ABC News story",
    "license": "MIT",
    "private": true,
    "contributors": [
        "{{authorName}} <{{authorEmail}}>"
    ],
    "aunty": {
        "deploy": {
            "contentftp": {
                "files": "**",
                "from": "build",
                "to": "/www/res/sites/news-projects/<name>/<id>"
            }
        }
    },
    "scripts": {
        "start": "aunty serve"
    },
    "devDependencies": {
        "@abcnews/aunty": "{{auntyVersion}}",
        "@abcnews/webpacker": "^1.0.0",
        "prettier": "^1.5.3"
    },
    "dependencies": {
        "babel-plugin-transform-react-jsx": "^6.24.1",
        "domready": "^1.0.8",
        "preact": "^8.2.1",
        "preact-compat": "^3.16.0",
        "smoothscroll": "^0.3.0"
    },
    "babel": {
        "plugins": [
            [
                "transform-react-jsx",
                {
                    "pragma": "h"
                }
            ]
        ]
    }
}